    "log": {
        "level": "debug",
        "timestamp": true
    },
    "experimental": {
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "ui",
            "secret": "",
            "external_ui_download_url": "https://codeload.github.com/MetaCubeX/metacubexd/zip/refs/heads/gh-pages",
            "external_ui_download_detour": "proxy",
            "default_mode": "rule"
        },
        "cache_file": {
            "enabled": true,
            "store_fakeip": true,
            "store_rdrc": true
        }
    },
    "dns": {
        "servers": [
            {
                "tag": "proxyDns",
                "address": "https://cloudflare-dns.com/dns-query#h3=true",
                "strategy": "ipv4_only",
                "address_resolver": "localDns",
                "client_subnet": "1.2.4.8/32",
                "detour": "proxy"
            },
            {
                "tag": "localDns",
                "address": "h3://223.6.6.6/dns-query",
                "strategy": "prefer_ipv6",
                "detour": "direct"
            },
            {
                "tag": "local_area_Dns",
                "address": "local",
                "detour": "direct"
            },
            {
                "tag": "nblock",
                "address": "rcode://refused"
            },
            {
                "tag": "dns_fakeip",
                "address": "fakeip",
                "strategy": "prefer_ipv6"
            }
        ],
        "rules": [
            {
                "domain_suffix": [
                    "mirror.ghproxy.com",
                    "cdn.jsdmirror.cn",
                    "fastly.jsdelivr.net"
                ],
                "server": "localDns"
            },
            {
                "domain_keyword": [
                    "ctgpaas",
                    "srdcloud",
                    "gdtel"
                ],
                "server": "local_area_Dns"
            },
            {
                "outbound": "any",
                "server": "localDns",
                "disable_cache": true
            },
            {
                "clash_mode": "direct",
                "server": "localDns"
            },
            {
                "clash_mode": "global",
                "server": "dns_fakeip"
            },
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "rule_set": "geosite-geolocation-!cn",
                        "invert": true
                    },
                    {
                        "rule_set": "geosite-geolocation-cn",
                        "invert": true
                    },
                    {
                        "rule_set": "geosite-cn",
                        "invert": true
                    }
                ],
                "server": "proxyDns",
                "disable_cache": true
            },
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "query_type": [
                            "A",
                            "CNAME"
                        ],
                        "rule_set": [
                            "geosite-cn",
                            "geosite-geolocation-cn"
                        ]
                    }
                ],
                "server": "localDns",
                "disable_cache": true
            },
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "query_type": [
                            "A",
                            "AAAA"
                        ]
                    }
                ],
                "server": "dns_fakeip",
                "rewrite_ttl": 30
            }
        ],
        "independent_cache": true,
        "final": "localDns",
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18"
        }
    },
